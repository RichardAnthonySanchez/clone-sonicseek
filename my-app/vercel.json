{
    "builds": [
        {
          "src": "../api/server.js",
          "use": "@vercel/node"
        },
        {
          "src": "my-app/**",
          "use": "@vercel/static"
        }
      ],

    "rewrites": [
        {
            "source": "/similar-artists",
            "destination": "../api/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/public/index.html"
        }
    ]
}